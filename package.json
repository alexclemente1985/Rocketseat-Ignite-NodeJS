{
  "name": "rentx",
  "version": "1.0.0",
  "description": "Treinamento Rocketseat NodeJS",
  "main": "index.js",
  "author": "Alexandre Clemente Pinheiro",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "docker:build": "docker build -t rentx .",
    "docker:run": "docker run -p 3333:3333 --name rentx-container rentx",
    "docker:logs": "docker logs rentx-container -f",
    "docker:start": "docker start rentx-container",
    "docker:stop": "docker stop rentx-container",
    "docker:rmcontainer": "docker container rm rentx-container",
    "docker:rmimage": "docker image rm rentx",
    "docker:exec": "docker exec -it rentx-container /bin/bash",
    "dcompose:up": "docker-compose up --force-recreate",
    "dcompose:down": "docker-compose down",
    "dcompose:start": "docker-compose start",
    "dcompose:stop": "docker-compose stop",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "test": "yarn remove bcrypt && yarn add bcrypt && jest"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "csv-parse": "^5.0.4",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "typeorm": "0.2.45",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.24",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
