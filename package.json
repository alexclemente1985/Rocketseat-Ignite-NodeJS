{
  "name": "rentalx",
  "version": "1.0.0",
  "description": "Servidor para aulas Ignite NodeJS Rocketseat",
  "main": "index.js",
  "author": "Alexandre C. Pinheiro",
  "license": "MIT",
  "scripts": {
    "docker:build": "docker build -t rentalx .",
    "docker:run": "docker run --name rentalx-container -p 3333:3333 rentalx",
    "docker:exec": "docker exec -it rentalx-container /bin/bash",
    "docker:start": "docker container start rentalx-container",
    "docker:stop": "docker container stop rentalx-container",
    "docker:logs": "docker logs rentalx-container -f",
    "start": "yarn tsc && node dist/server.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts"
  },
  "dependencies": {
    "csv-parse": "^5.0.4",
    "express": "^4.17.3",
    "multer": "^1.4.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
