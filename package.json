{
  "name": "rentx",
  "version": "1.0.0",
  "description": "Treinamento Rocketseat NodeJS",
  "main": "index.js",
  "author": "Alexandre Clemente Pinheiro",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "docker:build": "docker build -t rentx .",
    "docker:run": "docker run -p 3333:3333 --name rentx-container rentx",
    "docker:logs": "docker logs rentx-container -f",
    "docker:start":"docker start rentx-container",
    "docker:stop": "docker stop rentx-container",
    "docker:rmcontainer": "docker container rm rentx-container",
    "docker:rmimage": "docker image rm rentx",
    "docker:exec": "docker exec -it rentx-container /bin/bash",
    "dcompose:up" : "docker-compose up --force-recreate",
    "dcompose:down": "docker-compose down",
    "dcompose:start": "docker-compose start"
  },
  "dependencies": {
    "csv-parse": "^5.0.4",
    "express": "^4.17.3",
    "multer": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.24",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
